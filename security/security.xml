<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Security Groups -->
        <record id="group_cs_cart_manager" model="res.groups">
            <field name="name">CS-Cart Migration Manager</field>
            <field name="category_id" ref="base.module_category_hidden"/>
        </record>
        
        <record id="group_cs_cart_user" model="res.groups">
            <field name="name">CS-Cart Migration User</field>
            <field name="category_id" ref="base.module_category_hidden"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>
    </data>
</odoo>